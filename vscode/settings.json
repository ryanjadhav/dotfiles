// Place your settings in this file to overwrite the default settings
{
  "cssModules.camelCase": true,
  "editor.formatOnType": false,
  "editor.renderWhitespace": "all",
  "editor.scrollBeyondLastLine": false,
  "editor.minimap.enabled": false,
  "eslint.nodePath": "/usr/local/bin/node",
  "editor.renderControlCharacters": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 3000,
  "files.insertFinalNewline": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true,
    "**/.dist/**": true
  },
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "gitlens.keymap": "none",
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "html.format.endWithNewline": true,
  "javascript.validate.enable": false,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.dist": true,
    "**/.bundle.js": true,
    "app/public/scripts": true,
    "**/*.pyc": true,
    "**/.coverage": true,
    "**/generated": true
  },
  "search.useRipgrep": false,
  "workbench.activityBar.visible": true,
  "window.zoomLevel": 0,
  "terminal.integrated.rendererType": "dom",
  "workbench.sideBar.location": "left",
  "window.title": "${activeEditorLong}${separator}${rootName}",
  "jest.showCoverageOnLoad": true,
  "jest.enableCodeLens": true,
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.quoteStyle": "single",
  "typescript.updateImportsOnFileMove.enabled": "prompt",
  "typescript.tsdk": "/usr/local/lib/node_modules/typescript/lib",
  "diffEditor.renderSideBySide": false,
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "editor.fontWeight": "300",
  "breadcrumbs.enabled": true,
  "editor.multiCursorModifier": "alt",
  "workbench.startupEditor": "welcomePage",
  "editor.formatOnSave": true,
  "python.venvPath": "${workspaceFolder}/venv/bin/",
  "python.pythonPath": "venv/bin/python",
  "python.linting.flake8Args": [
    "--ignore=E3,E123,E226,E24,W391,W503",
    "--max-line-length=120"
  ],
  "python.linting.mypyEnabled": true,
  "python.linting.pycodestylePath": "venv/bin/pep8",
  "python.linting.pycodestyleEnabled": false,
  "python.linting.pycodestyleArgs": [
    "--ignore=E3,E123,E226,E24,W391,W503",
    "--max-line-length=120"
  ],
  "python.testing.unittestEnabled": false,
  "python.testing.nosetestsEnabled": false,
  "python.testing.pytestEnabled": true,
  "python.testing.pytestArgs": ["tests/unit", "--no-cov"],
  "workbench.colorTheme": "Community Material Theme Darker High Contrast",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "jest.enableInlineErrorMessages": true,
  "jest.autoEnable": false,
  "jest.coverageFormatter": "GutterFormatter",
  "jest.pathToJest": "npm run test --",
  "jest.restartJestOnSnapshotUpdate": true,
  "gitlens.currentLine.enabled": false,
  "gitlens.codeLens.scopes": ["document", "containers"],
  "gitlens.views.repositories.enabled": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "javascript.preferences.importModuleSpecifierEnding": "minimal",
  "typescript.preferences.importModuleSpecifierEnding": "minimal",
  "workbench.colorCustomizations": {
    "activityBarBadge.background": "#FFA000",
    "activityBar.activeBorder": "#FFA000",
    "list.activeSelectionForeground": "#FFA000",
    "list.inactiveSelectionForeground": "#FFA000",
    "list.highlightForeground": "#FFA000",
    "scrollbarSlider.activeBackground": "#FFA00050",
    "editorSuggestWidget.highlightForeground": "#FFA000",
    "textLink.foreground": "#FFA000",
    "progressBar.background": "#FFA000",
    "pickerGroup.foreground": "#FFA000",
    "tab.activeBorder": "#FFA000",
    "notificationLink.foreground": "#FFA000",
    "editorWidget.resizeBorder": "#FFA000",
    "editorWidget.border": "#FFA000",
    "settings.modifiedItemIndicator": "#FFA000",
    "settings.headerForeground": "#FFA000",
    "panelTitle.activeBorder": "#FFA000",
    "breadcrumb.activeSelectionForeground": "#FFA000",
    "menu.selectionForeground": "#FFA000",
    "menubar.selectionForeground": "#FFA000",
    "editor.findMatchBorder": "#FFA000",
    "selection.background": "#FFA00040"
  },
  "materialTheme.accent": "Yellow",
  "workbench.iconTheme": "eq-material-theme-icons",
  "python.languageServer": "Jedi",
  "workbench.preferredLightColorTheme": "Community Material Theme Ocean High Contrast",
  "workbench.preferredDarkColorTheme": "Community Material Theme Darker High Contrast",
  "yaml.schemas": {
    "file:///Users/rjadhav/.vscode/extensions/atlassian.atlascode-2.6.6/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "atlascode.jira.enabled": true,
  "atlascode.bitbucket.enabled": false
}
